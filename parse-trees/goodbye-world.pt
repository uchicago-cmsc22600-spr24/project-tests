(Prog (ValBind (VarPat hello) (StrExp "Hello")) (ValBind (VarPat goodbye) (StrExp "Goodbye"))
      (ValBind (VarPat world) (StrExp "world"))
      (ValBind WildPat
               (AppExp (VarExp print)
                       (AppExp (VarExp concat)
                               (TupleExp (VarExp hello)
                                         (AppExp (VarExp concat)
                                                 (TupleExp (StrExp ",\x20;")
                                                           (AppExp (VarExp concat)
                                                                   (TupleExp (VarExp world) (StrExp "!\n")))))))))
      (FunBind f ((VarPat n))
               (BindExp (FunBind loop ((VarPat i))
                                 (IfExp (BinExp (VarExp i) < (VarExp n))
                                        (BindExp (DoExpBind (AppExp (VarExp print) (StrExp "\x20;cruel")))
                                                 (AppExp (VarExp loop) (BinExp (VarExp i) + (IntExp 1)))) (TupleExp)))
                        (BindExp (DoExpBind (AppExp (VarExp print)
                                                    (AppExp (VarExp concat) (TupleExp (VarExp goodbye) (StrExp ",")))))
                                 (BindExp (DoExpBind (AppExp (VarExp loop) (IntExp 0)))
                                          (AppExp (VarExp print)
                                                  (AppExp (VarExp concat)
                                                          (TupleExp (StrExp "\x20;")
                                                                    (AppExp (VarExp concat)
                                                                            (TupleExp (VarExp world) (StrExp "!\n"))))))))))
      (DoExpBind (AppExp (VarExp f) (IntExp 5))))
