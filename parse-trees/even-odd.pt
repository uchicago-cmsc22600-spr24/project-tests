(Prog (FunBind intToString ((VarPat i))
               (IfExp (BinExp (VarExp i) < (IntExp 0))
                      (BinExp (StrExp "-") + (AppExp (VarExp intToString) (UnExp - (VarExp i))))
                      (IfExp (BinExp (VarExp i) == (IntExp 0)) (StrExp "0")
                             (IfExp (BinExp (VarExp i) == (IntExp 1)) (StrExp "1")
                                    (IfExp (BinExp (VarExp i) == (IntExp 2)) (StrExp "2")
                                           (IfExp (BinExp (VarExp i) == (IntExp 3)) (StrExp "3")
                                                  (IfExp (BinExp (VarExp i) == (IntExp 4)) (StrExp "4")
                                                         (IfExp (BinExp (VarExp i) == (IntExp 5)) (StrExp "5")
                                                                (IfExp (BinExp (VarExp i) == (IntExp 6)) (StrExp "6")
                                                                       (IfExp (BinExp (VarExp i) == (IntExp 7))
                                                                              (StrExp "7")
                                                                              (IfExp (BinExp (VarExp i) == (IntExp 8))
                                                                                     (StrExp "8")
                                                                                     (IfExp (BinExp (VarExp i) ==
                                                                                                    (IntExp 9))
                                                                                            (StrExp "9")
                                                                                            (BinExp (AppExp (VarExp intToString)
                                                                                                            (BinExp (VarExp i)
                                                                                                                    /
                                                                                                                    (IntExp 10)))
                                                                                                    +
                                                                                                    (AppExp (VarExp intToString)
                                                                                                            (BinExp (VarExp i)
                                                                                                                    %
                                                                                                                    (IntExp 10))))))))))))))))
      (FunBind boolToString ((VarPat b)) (IfExp (VarExp b) (StrExp "True") (StrExp "False")))
      (InstanceDcl toString (OvldTy () (FunTy (ConTy Int) (ConTy String))) intToString)
      (InstanceDcl toString (OvldTy () (FunTy (ConTy Bool) (ConTy String))) boolToString)
      (ValBind (VarPat zero) (IntExp 0)) (ValBind (VarPat one) (IntExp 1))
      (FunBind preEven ((VarPat odd) (VarPat x))
               (IfExp (BinExp (VarExp x) < (VarExp zero))
                      (AppExp (AppExp (VarExp preEven) (VarExp odd)) (UnExp - (VarExp x)))
                      (IfExp (BinExp (VarExp x) == (IntExp 0)) (ConExp True)
                             (AppExp (VarExp odd) (BinExp (VarExp x) - (VarExp one))))))
      (FunBind odd ((VarPat x))
               (IfExp (BinExp (VarExp x) < (VarExp zero)) (AppExp (VarExp odd) (UnExp - (VarExp x)))
                      (IfExp (BinExp (VarExp x) == (IntExp 0)) (ConExp False)
                             (AppExp (AppExp (VarExp preEven) (VarExp odd)) (BinExp (VarExp x) - (VarExp one))))))
      (ValBind (VarPat even) (AppExp (VarExp preEven) (VarExp odd))) (ValBind (VarPat n) (IntExp 101))
      (ValBind (VarPat even_n) (AppExp (VarExp even) (VarExp n)))
      (DoExpBind (AppExp (VarExp print) (StrExp "even\x20;")))
      (DoExpBind (AppExp (VarExp print) (AppExp (VarExp toString) (VarExp n))))
      (DoExpBind (AppExp (VarExp print) (StrExp "\x20;=\x20;")))
      (DoExpBind (AppExp (VarExp print) (AppExp (VarExp toString) (VarExp even_n))))
      (DoExpBind (AppExp (VarExp print) (StrExp "\n"))))
